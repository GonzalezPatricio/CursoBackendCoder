<h2 style="color: blue;">Listado</h2>

<% if(productos.length > 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: goldenrod;">
            <th>Title</th>
            <th>Price</th>
            <th>thumbnail</th>
            <th>Id</th>
            </tr>
            <% for (const productos of productos) {%>
            <tr>
                <td><%=productos.title %></td>
                <td><%=productos.price</td>
                <td><%=productos.thumbnail</td>
                <td><%=productos.id</td>
            </tr>
            <%}; %>
        </table>
    </div>

        <% }else{ %>
            <h3 class="alert alert-danger">No se encuentra el Producto</h3>
        <% } %>
}